{%- for block in mega_menu -%}
{%- capture css -%}
{%- if block.settings.color_background != "rgba(0,0,0,0)" -%}#{{ block.id }}.megamenu-container .submenu-design__megamenu{background:{{ block.settings.color_background }};}{%- endif -%}
{%- if block.settings.color_shadow     != "rgba(0,0,0,0)" and settings.show_shadow_header -%}#{{ block.id }}.megamenu-container{
{%- assign shadow_opacity = settings.shadow_opacity_header | divided_by: 100.00 -%}
--shadow-header:{{ settings.horizontal_length_header }}px {{ settings.vertical_length_header }}px {{ settings.blur_radius_header }}px 0 {{ block.settings.color_shadow | color_modify: 'alpha', shadow_opacity }};
}
{%- endif -%}
{%- if block.settings.color_heading != "rgba(0,0,0,0)" -%}#{{ block.id }}.megamenu-container .submenu__item__level-2{--color-submenu-text-block:{{ block.settings.color_heading }};}{%- endif -%}
{%- if block.settings.color_heading_hover != "rgba(0,0,0,0)" -%}
#{{ block.id }}.megamenu-container .submenu__item__level-2{
  --color-submenu-text-block-hover:{{ block.settings.color_heading_hover }};
  --color-text--color-submenu-text-block:{{ block.settings.color_heading_hover }};
}
{%- endif -%}
{%- if block.settings.color_link != "rgba(0,0,0,0)" -%}#{{ block.id }}.megamenu-container .megamenu-childs{--color-submenu-text-block:{{ block.settings.color_link }};}{%- endif -%}
{%- if block.settings.color_link_hover != "rgba(0,0,0,0)" -%}#{{ block.id }}.megamenu-container .megamenu-childs{--color-submenu-text-block-hover:{{ block.settings.color_link_hover }};}{%- endif -%}
{%- endcapture -%}
{%- if css != blank -%}
<style>{{- css -}}</style>
{%- endif -%}
{%- endfor -%}

