{%- if section.settings.swipe -%}
{{ 'swiper-bundle.min.css' | asset_url | stylesheet_tag }}
{{ 'component-swiper-slider.css' | asset_url | stylesheet_tag }}
{%- endif -%}

{%- capture section_styles -%}
{%- capture __css -%}
{%- if section.settings.color_background_box != "rgba(0,0,0,0)" -%}--color-box-background-block:{{ section.settings.color_background_box }};{%- endif -%}
{%- if section.settings.color_base   != "rgba(0,0,0,0)" -%}--color-base-section:{{ section.settings.color_base }};color:{{ section.settings.color_base }};{%- endif -%}
{%- if section.settings.color_accent != "rgba(0,0,0,0)" -%}--color-accent-section:{{ section.settings.color_accent }};{%- endif -%}
{%- if section.settings.color_lines  != "rgba(0,0,0,0)" -%}--color-lines:{{ section.settings.color_lines }};{%- endif -%}
{%- if section.settings.color_text_icons  != "rgba(0,0,0,0)" -%}--color-text-icons-block:{{ section.settings.color_text_icons }};{%- endif -%}
{%- if section.settings.color_desc != "rgba(0,0,0,0)" -%}--section-text-color:{{ section.settings.color_desc }};{%- endif -%}
{%- if section.settings.color_link  != "rgba(0,0,0,0)" -%}--color-link-button-block:{{ section.settings.color_link }};{%- endif -%}
{%- if section.settings.color_link_hover  != "rgba(0,0,0,0)" -%}--color-link-button-block-hover:{{ section.settings.color_link_hover }};{%- endif -%}
{%- if section.settings.color_link_icon  != "rgba(0,0,0,0)" -%}--color-link-button-icon-block:{{ section.settings.color_link_icon }};{%- endif -%}
{%- if section.settings.color_link_icon_hover  != "rgba(0,0,0,0)" -%}--color-link-button-icon-block-hover:{{ section.settings.color_link_icon_hover }};{%- endif -%}  
{%- if section.settings.color_text_link   != "rgba(0,0,0,0)" -%}--color-heading-link-block:{{ section.settings.color_text_link }};{%- endif -%}
{%- if section.settings.color_text_link_hover != "rgba(0,0,0,0)" -%}--color-heading-hover-block:{{ section.settings.color_text_link_hover }};{%- endif -%}
{%- if section.settings.color_media_arrows    != "rgba(0,0,0,0)" -%}--swiper-pagination-arrow-block:{{ section.settings.color_media_arrows }};{%- endif -%}
{%- if section.settings.color_media_dots      != "rgba(0,0,0,0)" -%}--swiper-pagination-bullet-block:{{ section.settings.color_media_dots }};{%- endif -%}
{%- if section.settings.color_text_icons_bg   != "rgba(0,0,0,0)" -%}--color-secondary-background-block:{{ section.settings.color_text_icons_bg }};{%- endif -%}
  
{%- endcapture -%}
{%- if __css != blank -%}
#theme-section-{{ section.id }}{
  {{ __css }}
}
{%- endif -%}
{%- endcapture -%}
<style>{{ section_styles }}
  .section-text-with-icons-type-1{
    display:flex;
    position: relative;
    flex-direction:column;
    justify-content: space-between;
    color:var(--section-text-color, var(--color-base-section, var(--color-base)));
    {% if section.settings.textbox %}background: var(--color-box-background-block, var(--color-body-background));{% endif %}
  }
  .section-text-with-icons-type-1.padding{
    padding:25px;
    border-radius:var(--radius);
    border:1px solid var(--color-lines);
  }
  #theme-section-{{ section.id }}{
    --icon-image-width: {{ section.settings.icon_width }}px;
  }
  #theme-section-{{ section.id }} .section-text-with-icons-type-1__icon svg{
    width: {{ section.settings.icon_width }}px;
    height: {{ section.settings.icon_width }}px;
  }
  #theme-section-{{ section.id }} .section-text-with-icons-type-1__icon__bg{
    padding:15px;
    border-radius:{{ section.settings.icon_box_roundness }}px;
    background:var(--color-secondary-background-block, var(--color-secondary-background));
  }
  swiper-slider .section-text-with-icons-type-1{
    height:100%;
  }
  .text-center .section-text-with-icons-type-1__button{
    justify-content: center;
  }
</style>
{%- render "section-css-background", section:section -%}
{%- liquid
    if template == "index" and section.settings.top_spacing_none_index == true
    echo '<div class="index-top-spacing-none"></div>'
    endif
-%}
{%- render "section-component-top-line", section:section -%}
{%- capture page_width -%}{%- if section.settings.width != "default" %}{{ section.settings.width }}{%- else -%}{{ settings.page_width_main }}{%- endif -%}{%- endcapture -%}
{%- assign section_settings_background = false -%}{%- if section.settings.background != 'hide' and section.settings.background_width == "content_width" -%}{%- assign section_settings_background = true -%}{%- endif -%}
<div id="theme-section-{{ section.id }}"
     class="{% if section.settings.show_lines == false or section.settings.background == 'hide' %}top-spacing-{{ section.settings.top_spacing }}{% endif %}{% if section.settings.background != 'hide' and section.settings.background_width == "fullwidth" %} section-background section-background--{{ section.settings.background_spacing }}{% endif %}"
     data-section-name="text-columns-with-icons-type-1">
  
  <div class="{{ page_width }}{% if section_settings_background %} section-background--mobile-width{% endif %}">
  {%- if section_settings_background -%}<div class="section-background section-background--{{ section.settings.background_spacing }} border__radius">{%- endif -%}
    {%- if section.settings.background != 'show' and section.settings.show_parallax -%}<div class="section-background__elements"><div class="section-background__elements__item"></div></div>{%- endif -%}
    {%- render "section-heading" section:section -%}

    {%- capture _content -%}
    {%- for block in section.blocks -%}
    {%- if section.settings.swipe -%}<div class="swiper-slide" lazy="true">{%- endif -%}
    <div id="theme-block-{{ block.id }}" class="section-text-with-icons-type-1{% if section.settings.textbox %} padding{% endif %} mt--first-child-0 {{ section.settings.text_align }}" {{ block.shopify_attributes }}>
      <div class="section-text-with-icons-type-1__block mt--first-child-0 mb--last-child-0">

        {%- capture icon_content -%}
        {%- render "component-image-icon",
            show_icon: block.settings.show_icon,
            image: block.settings.image,
            icon: block.settings.icon_name,
            image_custom_width: section.settings.icon_width,
        -%}
        {%- endcapture -%}
      
        {%- if icon_content != blank -%}
          <div class="section-text-with-icons-type-1__icon mb16 display-flex{% if section.settings.text_align == "text-center" %} justify-content-center{% endif %}">
            <div class="display-inline-flex{% if section.settings.show_icon_box %} section-text-with-icons-type-1__icon__bg{% endif %}">
              {{ icon_content }}
            </div>
          </div>
       {%- endif -%}
        {%- if block.settings.block_heading != blank -%}
          {%- if block.settings.url != blank -%}
          <{{ section.settings.heading_size }} class="mt0">
            <a href="{{ block.settings.url }}" class="clear-underline heading-color-block-hover{% if section.settings.hover_underline %} animation-underline{% endif %}">{{ block.settings.block_heading }}</a>
          </{{ section.settings.heading_size }}>
          {%- else -%}<{{ section.settings.heading_size }} class="mt0 heading-color-block-main">{{ block.settings.block_heading }}</{{ section.settings.heading_size }}>
          {%- endif -%}
        {%- endif -%}
        {%- if block.settings.block_text != blank -%}<div class="mt10 body2 section__richtext__editor">{{ block.settings.block_text }}</div>{%- endif -%}
      </div>
      {%- if block.settings.button_name != blank -%}
      <div class="section-text-with-icons-type-1__button mt16 display-inline-flex">
        <a{% if block.settings.url != blank %} href="{{ block.settings.url }}"{% endif %}
           class="link link-hover-span body3{% if settings.buttons_uppercase %} text-uppercase{% endif %}"
           {% if block.settings.url == blank %} aria-disabled="true"{% endif %}
          >
          <span>{{ block.settings.button_name }}</span>
          {{- 'icon-arrow-right-small' | append: settings.system_icon_thickness | inline_asset_content -}}
        </a>
      </div>
      {%- endif -%}
    </div>
    {%- if section.settings.swipe -%}</div>{%- endif -%}
    {%- endfor -%}
    {%- endcapture -%}
    

    {%- if section.settings.swipe -%}
    <div class="page-grid-1 no-row-gap swiper-no-mask-mobile">
      {%- assign data_grid_xl = section.settings.grid | split: "page-grid-" | last -%}
      {%- assign data_tablet_grid_lg = section.settings.grid_tablet | split: "page-grid-st-" | last -%}
      {%- assign data_grid = "1," | append: data_tablet_grid_lg | append: "," | append:data_grid_xl | append: "," | append:data_grid_xl | append: "," | append:data_grid_xl -%}
      <swiper-slider class="swiper{% if section.settings.swipe_edges == true %} swiper-no-mask{% endif %}"
                     data-swiper-desktop-grid="{{ data_grid_xl }}"
                     data-swiper-tablet-grid="{{ data_tablet_grid_lg }}"
                     data-gutter="20"
                     data-gutter-mobile="15"
                     data-grid="{{ data_grid }}"
                     {% if section.settings.autoplay_enabled and request.design_mode == false %} data-autoplay-delay="{{ section.settings.autoplay_time | times: 1000 }}"{% endif %}
                     data-autoplay="{%- if section.settings.autoplay_enabled and request.design_mode == false -%}true{%- else -%}false{%- endif -%}"
                     data-script="{{ 'swiper-bundle.min.js' | asset_url }}"
        >
        <div class="swiper-wrapper swiper-wrapper--max-height">
          {{ _content }}
        </div>
      </swiper-slider>
      <div class="swiper-control-container{% if section.settings.heading_aligment == "text-center" %} justify-content-center{% endif %}">
        <div class="custom-swiper-button-prev{% unless section.settings.desktop_navigation contains "arrows" %} hide-desktop{% endunless %}">{{- 'icon-arrow-left' | append: settings.system_icon_thickness | inline_asset_content -}}</div>
        <div class="swiper-pagination{% unless section.settings.desktop_navigation contains "dots" %} hide-desktop{% endunless %}{% unless section.settings.mobile_navigation contains "dots" %} hide-mobile-xl-st{% endunless %}"></div>
        <div class="custom-swiper-button-next{% unless section.settings.desktop_navigation contains "arrows" %} hide-desktop{% endunless %}">{{- 'icon-arrow-right' | append: settings.system_icon_thickness | inline_asset_content -}}</div>
      </div>
    </div>
    {%- else -%}
    <div class="page-grid-sp-1 {{ section.settings.grid_tablet }} {{ section.settings.grid }}{% if section.settings.textbox == false %} page-vertical-gap-40{% endif %}{% if section.settings.heading_aligment == "text-center" %} justify-content-center{% endif %}">
      {{ _content }}
    </div>
    {%- endif -%}


    {% if section_settings_background %}</div>{%- endif -%}
  </div>
</div>

