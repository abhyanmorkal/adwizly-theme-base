{%- render "component-header-style", section:section -%}
<style>
  .list-menu--classic .list-menu--inline{
    --row-gap: 10px;
    --column-gap: 20px;
  }
  @media (min-width:1025px){
    .header__main .list-menu{
      {%- if section.settings.color_menu_item != "rgba(0,0,0,0)" -%}--color-menu-block:{{ section.settings.color_menu_item }};{%- endif -%}
      {%- if section.settings.color_menu_item_hover != "rgba(0,0,0,0)" -%}--color-menu-block-hover:{{ section.settings.color_menu_item_hover }};{%- endif -%}
    }
    {%- if section.settings.logo_position == 'logo-to-center' and section.settings.classic_menu_position == 'top-menu-to-center' -%}
    .menu-center__logo-center__container{
      display: grid;
      grid-template-columns: 1fr auto 1fr;
      row-gap: 20px;
      column-gap: 30px;
    }
    .menu-center__logo-center__container>:first-child{
      justify-content:flex-end;
    }
    .menu-center__logo-center__container>:last-child{
      justify-content:flex-start;
    }
    .menuwith-logo__2floor{
      grid-template-columns: 1fr 1fr;
      grid-template-rows: auto auto;
      grid-template-areas: 
          "col1 col2"
          "full full";
      padding-bottom:20px;
    }
    .menuwith-logo__2floor > :nth-child(1) { grid-area: col1; min-height:40px; }
    .menuwith-logo__2floor > :nth-child(3) { grid-area: col2; min-height:40px; }
    .menuwith-logo__2floor > :nth-child(2) { grid-area: full; }
    {%- endif -%}
  }
</style>

{%- if section.settings.enable__sticky_header != "none" and template.name != 'product' and template.name != 'collection' -%}
<sticky-header class="display-flex sticky-header{% if section.settings.sticky_type == "scroll" %} sticky-header-scroll{% endif %} enable-on__{{ section.settings.enable__sticky_header }}"></sticky-header>
{%- endif -%}
{%- render "section-header-1", section:section -%}
{%- render "component-header-search-modal", section:section -%}
{%- render "component-header-interface", section:section -%}
{%- if section.settings.logo_position == 'logo-to-center' and section.settings.classic_menu_position == 'top-menu-to-center' -%}
<script>
class MenuwithLogo extends HTMLElement {
  constructor() {
    super();
  }
  connectedCallback() {
    window.addEventListener('resize', this.resizeHandler.bind(this));
    if(Shopify.designMode ){
      setTimeout(this.resizeHandler.bind(this), 300);
    }
    else{
      document.addEventListener('DOMContentLoaded', this.resizeHandler.bind(this));
    }
  }
  resizeHandler(){
    if(window.innerWidth <= 1024) return false;
    var _parent = this.parentNode;
    _parent.classList.remove('menuwith-logo__2floor');
    var child1_w = _parent.children[0].offsetWidth;
    var child3_w = _parent.children[2].offsetWidth;
    var _w = Array.from(_parent.children[2].children).reduce((sum, child) => sum + child.offsetWidth, 0) + (parseFloat(getComputedStyle(_parent).columnGap) || 0) + (_parent.children[2].children.length - 1);
    if(_w > child3_w || child1_w != child3_w){
      _parent.classList.add('menuwith-logo__2floor');
    }
  }
}
customElements.define('menuwith-logo', MenuwithLogo);
</script>
{%- endif -%}

