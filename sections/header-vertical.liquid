{%- render "component-header-style", section:section -%}
{{ 'component-vertical-menu.css' | asset_url | stylesheet_tag }}
<style>
{%- assign sections_count = section.settings.sections_count | plus: 0 -%}
{% if sections_count > 0 and template == 'index' %}
@media (min-width:1025px){
  fixed-headergroup:has([data-menu-opened])~#MainContent>*:first-child>fullscreen-object-collage>div [class*="page-wi"],
  fixed-headergroup:has([data-menu-opened])~#MainContent>*:first-child>[class*="page-wi"].background__page-padding0,
  fixed-headergroup:has([data-menu-opened])~#MainContent>*:first-child>div [class*="page-wi"],
  .section-header:has([data-menu-opened])~#MainContent>*:first-child>fullscreen-object-collage>div [class*="page-wi"],
  .section-header:has([data-menu-opened])~#MainContent>*:first-child>[class*="page-wi"].background__page-padding0,
  .section-header:has([data-menu-opened])~#MainContent>*:first-child>div [class*="page-wi"]
  {%- if sections_count > 1 -%},
  fixed-headergroup:has([data-menu-opened="2"])~#MainContent>*:nth-child(2)>fullscreen-object-collage>div [class*="page-wi"],
  fixed-headergroup:has([data-menu-opened="2"])~#MainContent>*:nth-child(2)>[class*="page-wi"].background__page-padding0,
  fixed-headergroup:has([data-menu-opened="2"])~#MainContent>*:nth-child(2)>div [class*="page-wi"],
  .section-header:has([data-menu-opened="2"])~#MainContent>*:nth-child(2)>fullscreen-object-collage>div [class*="page-wi"],
  .section-header:has([data-menu-opened="2"])~#MainContent>*:nth-child(2)>[class*="page-wi"].background__page-padding0,
  .section-header:has([data-menu-opened="2"])~#MainContent>*:nth-child(2)>div [class*="page-wi"]{%- endif -%}
  {
    padding-left: calc(270px + var(--grid-padding) + var(--page-padding));
  }
}
{%- endif -%}
header{
  --submenu-correction:{{ section.settings.submenu_correction }}px;
  --icons_padding:{{ section.settings.icons_padding }}px;
  --items-padding:{{ section.settings.items_padding }}px;
  --megamenu_correction:{{ section.settings.megamenu_correction }}px;
}
.header{
  min-height:70px;
  padding-top:20px;
  padding-bottom:20px;
}
.header-phone-number__desktop__container {
  max-width: 270px;
}
.header-phone-number__desktop__left .header-phone-number__desktop{
  text-align:left;
  align-items: flex-start;
}
.header-phone-number__desktop__left .header-phone-number__desktop_submenu{
  right:auto;
  left:0;
}
[class*=page-grid-].page-grid__header-sidebar>*:first-child {
  max-width: 270px;
}
[class*=page-grid-].page-grid__header-sidebar>*:last-child {
  max-width: calc(100% - 270px - var(--page-padding));
}
.page-grid__header-sidebar{
  padding-bottom:20px;
}
.vertical-menu__items .vertical-menu-item img,
.vertical-menu-item svg:first-child {
  --icon-width:{{ section.settings.icons_size }}px;
  --icon-height:{{ section.settings.icons_size }}px;
}
{%- if section.settings.color_menu_item != "rgba(0,0,0,0)" -%}.header__desktop .vertical-menu__button{--background:{{ section.settings.color_menu_item }};}{%- endif -%}
{%- if section.settings.color_menu_item_text != "rgba(0,0,0,0)" -%}.header__desktop .vertical-menu__button{--color:{{ section.settings.color_menu_item_text }};}{%- endif -%}
{%- if section.settings.color_menu_lines != "rgba(0,0,0,0)" -%}.header__desktop .vertical-menu__container{--color-lines:{{ section.settings.color_menu_lines }};}{%- endif -%}
{%- if section.settings.color_menu_background != "rgba(0,0,0,0)" -%}.header__desktop .vertical-menu__container{--color-vertical-menu-dropdown:{{ section.settings.color_menu_background }};}{%- endif -%}
{%- if section.settings.color_menu_link != "rgba(0,0,0,0)" -%}
.header__desktop .vertical-menu-item{--color-text-link-block:{{ section.settings.color_menu_link }};}
.header__desktop .vertical-menu-item svg:last-child{--color-text-icons-block:{{ section.settings.color_menu_link }};}
{%- endif -%}
{%- if section.settings.color_menu_link_hover != "rgba(0,0,0,0)" -%}
.header__desktop .vertical-menu-item{--color-text-link-hover-block:{{ section.settings.color_menu_link_hover }};}
.header__desktop .vertical-menu-item svg:last-child{--color-text-icons-block-hover:{{ section.settings.color_menu_link_hover }};}
{%- endif -%}
{%- if section.settings.color_menu_link_icon != "rgba(0,0,0,0)" -%}.header__desktop .vertical-menu-item svg:first-child{--color-text-icons-block:{{ section.settings.color_menu_link_icon }};}{%- endif -%}
{%- if section.settings.color_menu_link_icon_hover != "rgba(0,0,0,0)" -%}.header__desktop .vertical-menu-item svg:first-child{--color-text-icons-block-hover:{{ section.settings.color_menu_link_icon_hover }};}{%- endif -%}
{%- if section.settings.color_menu_show_all != "rgba(0,0,0,0)" -%}
.header__desktop .vertical-menu__show-more{
  --color-link-button-block:{{ section.settings.color_menu_show_all }};
  --color-link-button-icon-block:{{ section.settings.color_menu_show_all }};
}
{%- endif -%}
{%- if section.settings.color_menu_show_all_hover != "rgba(0,0,0,0)" -%}
.header__desktop .vertical-menu__show-more{
  --color-link-button-block-hover:{{ section.settings.color_menu_show_all_hover }};
  --color-link-button-icon-block-hover:{{ section.settings.color_menu_show_all_hover }};
}
{%- endif -%}
{%- if settings.show_shadow_header == true -%}
{%- assign shadow_opacity = settings.shadow_opacity_header | divided_by: 100.00 -%}
.vertical-menu{
--menu-shadow: {{ settings.horizontal_length_header }}px {{ settings.vertical_length_header }}px {{ settings.blur_radius_header }}px 0 rgb(from {{ settings.color_shadow_header }} r g b / {{ shadow_opacity }});
}
{%- endif -%}
@media (min-width:1025px){
  .header__icons__customer__icons .header__button svg{
    width:{{ section.settings.icons_size_user }}px;
    height:{{ section.settings.icons_size_user }}px;
  }
  .header__icons__customer__icons .header__button__bubble{
    {%- if section.settings.icons_size_user > 30 -%}
    width: 24px;
    height: 24px;
    --left:calc({{ section.settings.icons_size_user }}px - 10px);
    {%- else -%}
    --left:calc({{ section.settings.icons_size_user }}px - 7px);
    {%- endif -%}
    left:var(--left);
  }
  .header__button.move-icon-up:hover .header__button__bubble {
    width:calc({{ section.settings.icons_size_user }}px + 10px);
    height:calc({{ section.settings.icons_size_user }}px + 10px);
  }
}
</style>
{%- if section.settings.enable__sticky_header != "none" and template.name != 'product' and template.name != 'collection' -%}
<sticky-headervertical class="display-flex sticky-header sticky-header-scroll enable-on__{{ section.settings.enable__sticky_header }}"></sticky-headervertical>
{%- endif -%}
{%- render "section-header-3", section:section -%}
{%- render "component-header-search-modal", section:section -%}
{%- render "component-header-interface", section:section -%}
<script src="{{ 'vertical-menu.js' | asset_url }}" defer="defer"></script>

